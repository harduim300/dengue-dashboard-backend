{
    "version": 2,
    "builds": [
      {
        "src": "./server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/dengue_api/version",
        "dest": "/server.js"
      },
      {
        "src": "/dengue_api/uf/:uf",
        "dest": "/server.js"
      },
      {
        "src": "/dengue_api/br",
        "dest": "/server.js"
      },
      {
        "src": "/dengue_api/grafico_br",
        "dest": "/server.js"
      },
      {
        "src": "/dengue_api/grafico_uf/:estado",
        "dest": "/server.js"
      }
    ]
  }
  